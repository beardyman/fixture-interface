{
  "name": "fixture-interface",
  "version": "1.0.0",
  "description": "A simple interface to make helpers for setting up and tearing down data needed for tests.",
  "main": "fixture.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "lodash": "4.17.5"
  },
  "devDependencies": {
    "jest": "22.1.4",
    "sinon": "4.2.2"
  },
  "scripts": {
    "test": "jest test/*.spec.js --watch --coverage --coverageDirectory='test/reports'",
    "example": "jest example/test --watch --coverage --coverageDirectory='example/test/reports'"
  },
  "keywords": [
    "test",
    "fixture"
  ],
  "author": "@beardyman",
  "license": "MIT"
}
